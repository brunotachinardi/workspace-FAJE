package com.eguru.faje.utils.sound.events {	import com.eguru.faje.utils.sound.core.ISoundObject;	import flash.events.Event;	/**	 * @author Matt Przybylski [http://www.reintroducing.com] 	 * @version 1.0 	 */	public class SoundObjectEvent extends Event	{		public static const SOUND_OBJECT_ADDED : String = "SoundObjectEvent.SoundObjectAdded";		public static const SOUND_OBJECT_REMOVED : String = "SoundObjectEvent.SoundObjectRemoved";		public static const SOUND_OBJECT_PLAY_START : String = "SoundObjectEvent.SoundObjectPlayStart";		public static const SOUND_OBJECT_PAUSE : String = "SoundObjectEvent.SoundObjectPause";		public static const SOUND_OBJECT_STOP : String = "SoundObjectEvent.SoundObjectStop";		public static const SOUND_OBJECT_MUTE : String = "SoundObjectEvent.SoundObjectMute";		public static const SOUND_OBJECT_UNMUTE : String = "SoundObjectEvent.SoundObjectUnmute";		public static const SOUND_OBJECT_PLAY_COMPLETE : String = "SoundObjectEvent.SoundObjectPlayComplete";		public static const SOUND_OBJECT_FADE : String = "SoundObjectEvent.SoundObjectFade";		public static const SOUND_OBJECT_FADE_COMPLETE : String = "SoundObjectEvent.SoundObjectFadeComplete";		public static const SOUND_OBJECT_LOAD_ERROR : String = "SoundObjectEvent.SoundObjectLoadError";		public static const SOUND_OBJECT_LOAD_PROGRESS : String = "SoundObjectEvent.SoundObjectLoadProgress";		public static const SOUND_OBJECT_LOAD_COMPLETE : String = "SoundObjectEvent.SoundObjectLoadComplete";		public static const REMOVED_ALL : String = "SoundObjectEvent.RemovedAll";		public static const PLAY_ALL : String = "SoundObjectEvent.PlayAll";		public static const STOP_ALL : String = "SoundObjectEvent.StopAll";		public static const PAUSE_ALL : String = "SoundObjectEvent.PauseAll";		public static const FADE_ALL : String = "SoundObjectEvent.FadeAll";		public static const MUTE_ALL : String = "SoundObjectEvent.MuteAll";		public static const UNMUTE_ALL : String = "SoundObjectEvent.UnmuteAll";				protected var _soundItem : ISoundObject;		protected var _duration : Number;		protected var _percent : Number;		public function SoundObjectEvent(type : String, soundItem : ISoundObject = null, duration : Number = 0, percent : Number = 0, bubbles : Boolean = false, cancelable : Boolean = false)		{			super(type, bubbles, cancelable);						_soundItem = soundItem;			_duration = duration;			_percent = percent;		}		public override function clone():Event		{			return new SoundObjectEvent(type, _soundItem, _duration, _percent, bubbles, cancelable);		}				public function get soundItem() : ISoundObject		{			return _soundItem;		}				public function get duration() : Number		{			return _duration;		}				public function get percent() : Number		{			return _percent;		}				public override function toString():String		{			return formatToString("SoundManagerEvent", "soundItem", "duration", "percent", "type", "bubbles", "cancelable");		}	}}